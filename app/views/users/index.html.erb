    }
    getEle("statistics").addEventListener("click",()=>{
        getEle("statistics").classList.toggle("show")
    })
    getEle("report").addEventListener("click",()=>{
        getEle("report").classList.toggle("show")
    })
        getEle("userIcon").addEventListener("click",()=>{
          getEle("userIcon").classList.toggle("color__primary")
          getEle("userNotifi").classList.toggle("Notify__block")
        })
      
  </script>
  <!-- Show option -->
  <script>
    const khoa = document.querySelectorAll(".khoa")
    getEle("select__khoa").addEventListener("click",()=>{
        getEle("option__khoa").classList.toggle("show__option")
        getEle("select__khoa").classList.toggle("radiusSelect")
    
    })
    
    khoa.forEach(e=>{
        e.addEventListener("click",(e)=>{
          getEle("selectedKhoaModal").innerHTML = e.target.innerText
        })
    })
    const bomon = document.querySelectorAll(".bomon")
    getEle("select__bomon").addEventListener("click",()=>{
        getEle("option__bomon").classList.toggle("show__option")
        getEle("select__bomon").classList.toggle("radiusSelect")
    
    })
    
    bomon.forEach(e=>{
        e.addEventListener("click",(e)=>{
          getEle("selectedBomonModal").innerHTML = e.target.innerText
        })
    })
    const vaitro = document.querySelectorAll(".vaitro")
    getEle("select_vaitro").addEventListener("click",()=>{
        getEle("option__vaitro").classList.toggle("show__option")
        getEle("select_vaitro").classList.toggle("radiusSelect")
    
    })
    
    vaitro.forEach(e=>{
        e.addEventListener("click",(e)=>{
          getEle("selectedVaitroModal").innerHTML = e.target.innerText
        })
    })
    const donvi = document.querySelectorAll(".donvi")
    getEle("select__donvi").addEventListener("click",()=>{
        getEle("option__donvi").classList.toggle("show__option")
        getEle("select__donvi").classList.toggle("radiusSelect")
    
    })
    
    donvi.forEach(e=>{
        e.addEventListener("click",(e)=>{
          getEle("setededDonvi").innerHTML = e.target.innerText
        })
    })
    
    getEle("btn__addmember").addEventListener("click",()=>{
        getEle("Modal__AddMember").classList.add("show__modal")
    })
    getEle("close__modalIcon").addEventListener("click",()=>{
        getEle("Modal__AddMember").classList.remove("show__modal")
    })
    getEle("btn__closeModal").addEventListener("click",()=>{
        getEle("Modal__AddMember").classList.remove("show__modal")
    })
  </script>
