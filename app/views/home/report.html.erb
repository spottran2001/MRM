    align-items: center;
    background-color: rgba(0, 0, 0, 0.5);
    display: none;
}
#popup__inner{
    background-color: white;
    width: 30%;
    border: 2px solid black;
    border-radius: 12px;
    overflow: hidden;
    box-shadow: 0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1);
    height: 36%;
}
.popup_header{
    display: flex;
