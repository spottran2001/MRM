/* MENU */
.menu {
  background-color: white;
  width: 100px;
  max-height: 100%;
  box-shadow: 0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1);
  transition: all 0.5s;
  display: block;
  position: relative;
}
.menu__title {
  text-align: center;
  font-size: 14px;
  font-weight: bold;
}
.menu__logo {
  width: 100%;
  padding: 20px;
}
.menu__logo img {
  width: 100%;
}
.menu__items {
  width: 100%;
  text-align: center;
  padding: 20px 0px 20px;
  cursor: pointer;
  color: #dadada;
  display: block;
}
.menu__items--separation {
  margin-bottom: 40px;
}
.menu__icon i {
  font-size: 20px;
}

.menu__icon {
  padding: 6px 0px;
}

.menu__active {
  border-left: 2px solid #d72134;
  color: #d72134 !important;
}

/* Content */

.content {
  background-color: #f5f5f5;
  width: 100%;
  padding: 30px 60px 20px 30px;
}
.content__title {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content__name {
  padding: 40px 0px 10px 40px;
  font-size: 30px;
}

.content__breadcrumb {
  font-size: 16px;
  font-weight: bold;
  margin-left: 20px;
}

.breadcrumb__name {
  display: inline-block;
  margin-left: 10px;
  color: #f34f68;
  border-bottom: 3px solid #f34f68;
  padding: 12px 0px;
}
.content__user {
  display: flex;
  align-items: center;
}
.user_icon {
  font-size: 24px;
  margin-right: 14px;
  margin-top: 6px;
}
.user__info {
  margin-right: 10px;
  font-size: 16px;
}
.user__info p:first-child {
  text-align: right;
}

.user__img img {
  border-radius: 50%;
  width: 40px;
  height: 100%;
}
.user__img {
  margin-right: 10px;
}
.user__detail {
  font-size: 20px;
  cursor: pointer;
  color: gray;
}

.content__generality {
  display: flex;
  align-items: center;
